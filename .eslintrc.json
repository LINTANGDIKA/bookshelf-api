{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-useless-constructor": "error",
        "camelcase": "off",
        "no-unused-vars": "warn",
        "react/react-in-jsx-scope": "off",
        "react/prop-types" : "off",
        "no-undef" : "off",
        "no-extra-semi" : "warn"
    }
}
